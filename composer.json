{
    "name": "mindbird/contao-appointment",
    "type": "contao-bundle",
    "description": "A new description is needed",
    "homepage": "https://www.mindbird.de",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "mindbird",
            "homepage": "https://github.com/mindbird"
        }
    ],
    "require": {
        "php": "^7.2",
        "contao/core-bundle": "4.9.*",
        "symfony/framework-bundle": "^4.4",
        "symfony/orm-pack": "^1.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Mindbird\\Contao\\Appointment\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Mindbird\\Contao\\Appointment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mindbird\\Contao\\Appointment\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/mindbird/contao-appointment/issues",
        "source": "https://github.com/mindbird/contao-appointment"
    }
}
